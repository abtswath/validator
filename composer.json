{
    "name": "abtswath/validator",
    "description": "A form verification library written in php8 attributes.",
    "type": "library",
    "require": {
        "php": "^8.0.2",
        "ext-mbstring": "*",
        "symfony/mime": "^6.0",
        "symfony/http-foundation": "^6.0",
        "egulias/email-validator": "^3.2",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Abtswath\\Validator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit"
    },
    "authors": [
        {
            "name": "abtswath",
